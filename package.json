{
  "name": "alphally-backend",
  "version": "1.0.0",
  "description": "This is a private directory for movies and events ticket sales",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src --fix",
    "test": "cross-env NODE_ENV=test nyc --reporter=text --reporter=html mocha src/test/*.js --timeout 10000 -c --require @babel/register --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "node dist/index.js",
    "build": "babel src -d dist",
    "migrate": "sequelize db:migrate ",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "migrate:seed": "npm run migrate && npm run seed",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset && cross-env NODE_ENV=test npm run migrate",
    "prestart": "npm run migrate",
    "start:dev": "nodemon --exec babel-node ./src/index.js",
    "test:dev": "node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tueloper/Alphally-Backend.git"
  },
  "keywords": [
    "Alphally",
    "Backend",
    "Movies",
    "Events",
    "Tickets"
  ],
  "author": "Tochukwu Ozurumnba (https://github.com/Tueloper)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tueloper/Alphally-Backend/issues"
  },
  "homepage": "https://github.com/Tueloper/Alphally-Backend#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@hapi/joi": "^15.1.1",
    "@sendgrid/mail": "^7.4.5",
    "@types/hapi__joi": "^15.0.3",
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.4",
    "cloudinary": "^1.25.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.15.6",
    "joi-password-complexity": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "methods": "^1.1.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nigerian-phone-number-validator": "^1.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.87.0",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "twilio": "^3.60.0",
    "underscore": "^1.13.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "faker": "^4.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.1.0"
  }
}
